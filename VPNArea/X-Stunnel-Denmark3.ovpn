client
proto tcp
dev tun0
remote 127.0.0.1 45453
tun0-mtu 1500
route 82.103.128.160 255.255.255.255 net_gateway
resolv-retry infinite
nobind
persist-key
persist-tun
ns-cert-type server
ca "ca.crt"
cipher AES-256-CBC
auth SHA256
verb 3
auth-user-pass
mute-replay-warnings
passtos
tun-mtu 1500
script-security 2
up /etc/openvpn/update-resolv-conf
down /etc/openvpn/update-resolv-conf
