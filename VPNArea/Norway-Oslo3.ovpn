client
proto udp
dev tun0
remote 185.90.61.3 53
resolv-retry infinite
nobind
persist-key
persist-tun
ns-cert-type server
ca "ca.crt"
cipher AES-256-CBC
auth SHA256
verb 3
auth-user-pass
passtos
tun-mtu 1500
mute-replay-warnings
script-security 2
up /etc/openvpn/update-resolv-conf
down /etc/openvpn/update-resolv-conf
