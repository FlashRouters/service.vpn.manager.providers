client
proto udp
dev tun0
remote 191.96.4.2 8292
resolv-retry infinite
nobind
persist-key
persist-tun
ns-cert-type server
ca "ca.crt"
cipher AES-256-CBC
auth SHA256
verb 3
auth-user-pass
mute-replay-warnings
passtos
tun-mtu 1500
script-security 2
up /etc/openvpn/update-resolv-conf
down /etc/openvpn/update-resolv-conf
